process {
    withName:tx2gene {
        executor='slurm'
        queue="${params.queue}"
        cpus=1
        memory='4 GB'
        time='1h'
        jobName={ "$task.name" }
        container = "${params.image_folder}rnaseq.python-3.8-1.sif"
        containerOptions = "-B ${params.project_folder}:/workdir"
    }
    withName:tx2gene_proc {
        executor='slurm'
        queue="${params.queue}"
        cpus=1
        memory='4 GB'
        time='1h'
        jobName={ "$task.name" }
        container = "${params.image_folder}rnaseq.python-3.8-1.sif"
        containerOptions = "-B ${params.project_folder}:/workdir"
    }
    withName:annotations {
        executor='slurm'
