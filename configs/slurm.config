process {
    withName:tx2gene {
        executor='slurm'
        queue="${params.queue}"
        cpus=1
        memory='4GB'
        time='1h'
        container = "${params.image_folder}rnaseq.python-3.8-5.sif"
        containerOptions = "${params.extra_mounts} -B ${params.project_folder}:/workdir"
    }
    withName:tx2gene_proc {
        executor='slurm'
        queue="${params.queue}"
        cpus=1
        memory='4GB'
        time='1h'
        container = "${params.image_folder}rnaseq.python-3.8-5.sif"
        containerOptions = "${params.extra_mounts} -B ${params.project_folder}:/workdir"
    }
    withName:annotations {
        executor='slurm'
        queue="${params.queue}"
