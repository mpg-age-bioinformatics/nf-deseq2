singularity.enabled = true
process {
    withName:tx2gene {
        executor='slurm'
        queue="${params.queue}"
        cpus=1
        mem='4 GB'
        time='1h'
        jobName={ "$task.name" }
        container = "${params.image_folder}rnaseq.python-3.8-1"
        containerOptions = "-B ${params.project_folder}:/workdir"
    },
    withName:annotations {
        executor='slurm'
        queue="${params.queue}"
        cpus=1
        mem='4 GB'
        time='1h'
        jobName={ "$task.name" }
        container = "${params.image_folder}rnaseq.python-3.8-1"
        containerOptions = "-B ${params.project_folder}:/workdir"
    },
    withName:parse_submission {
        executor='slurm'
        queue="${params.queue}"
        cpus=1
        mem='4 GB'
        time='1h'
        jobName={ "$task.name" }
        container = "${params.image_folder}rnaseq.python-3.8-1"
        containerOptions = "-B ${params.project_folder}:/workdir"
    }
}

