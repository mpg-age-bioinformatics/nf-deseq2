singularity.enabled = true
process {
    withName:string {
        executor='slurm'
        queue="${params.queue}"
        cpus=2
        mem='4 GB'
        time='1h'
        jobName={ "$task.name" }
        container = "${params.image_folder}rnaseq.python-3.8-1.sif"
        containerOptions = "-B ${params.project_folder}:/workdir"
    }
}

