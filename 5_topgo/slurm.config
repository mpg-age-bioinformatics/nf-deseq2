singularity.enabled = true
process {
    withName:topgo {
        executor='slurm'
        queue="${params.queue}"
        cpus=1
        mem='8 GB'
        time='4h'
        jobName={ "$task.name" }
        container = "${params.image_folder}topgo-2.50.0.sif"
        containerOptions = "-B ${params.project_folder}:/workdir"
    }
}

